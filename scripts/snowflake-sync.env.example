# Snowflake Sync Configuration
# Copy this to snowflake-sync.env and fill in your values

# MySQL/MariaDB Configuration
MYSQL_HOST=pbx-mariadb
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_root_password_here
MYSQL_DATABASE=asterisk

# Snowflake Configuration
SNOWFLAKE_ACCOUNT=your_account_identifier_here
SNOWFLAKE_USER=your_snowflake_username
SNOWFLAKE_PASSWORD=your_snowflake_password
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=FREEPBX_CDR
SNOWFLAKE_SCHEMA=CDR_DATA

# Sync Settings
SYNC_HOURS=24
